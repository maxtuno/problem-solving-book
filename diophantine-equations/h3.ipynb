   "source": [
    "tc = peqnp.TheCore(bits=32, deepness=16, key='h3', folder='/Volumes/RAM/')\n",
    "\n",
    "n = tc.int()\n",
    "x = tc.int()\n",
    "\n",
    "assert x**2 + 7 == tc.two**n\n",
    "\n",
    "while tc.satisfy([n, x]):\n",
    "    print(n, x)"
